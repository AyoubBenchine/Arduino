//****************************************************** INCLUDE ******************************************************



//****************************************************** VARIABLES ****************************************************
float  KgCO2m2 = 3;

//****************************************************** SETUP ********************************************************
void setup()              // run once, when the sketch starts
{
  Serial.begin(9600);     // set up Serial library at 9600 bps

  if (KgCO2m2 < 3.5)
  {
    Serial.println("A");
  }  
  else if (KgCO2m2 < 6.5)
  {
    Serial.println("B");
  }
  else if (KgCO2m2 < 11.1)
  {
    Serial.println("C");
  }
   else if (KgCO2m2 < 17.7)
  {
    Serial.println("D");
  }
  else if (KgCO2m2 < 38.2)
  {
    Serial.println("E");
  } 
  else if (KgCO2m2 < 43.2)
  {
    Serial.println("F");
  }
  else
  {
    Serial.println("G");
  }
}
